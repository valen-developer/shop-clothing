{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "clothing-shop-front": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/clothing-shop-front",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": [
              "src/styles.css",
              "src/app/auth/login/assets/fonts/iconic/css/material-design-iconic-font.min.css",
              "src/app/auth/login/assets/vendor/animate/animate.css",
              "src/app/auth/login/assets/vendor/css-hamburgers/hamburgers.min.css",
              "src/app/auth/login/assets/vendor/animsition/css/animsition.min.css",
              "src/app/auth/login/assets/vendor/select2/select2.min.css",
              "src/app/auth/login/assets/vendor/daterangepicker/daterangepicker.css",
              "src/app/auth/login/assets/css/util.css",
              "src/app/auth/login/assets/css/main.css"
            ],
            "scripts": [

              "src/app/auth/login/assets/vendor/animsition/js/animsition.min.js",
              "src/app/auth/login/assets/vendor/bootstrap/js/popper.js",
              "src/app/auth/login/assets/vendor/bootstrap/js/bootstrap.min.js",
              "src/app/auth/login/assets/vendor/select2/select2.min.js",
              "src/app/auth/login/assets/vendor/daterangepicker/daterangepicker.js",
              "src/app/auth/login/assets/vendor/countdowntime/countdowntime.js",
              "src/app/auth/login/assets/js/main.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "clothing-shop-front:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "clothing-shop-front:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "clothing-shop-front:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": ["src/styles.css"],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "clothing-shop-front:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "clothing-shop-front:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "clothing-shop-front"
}
